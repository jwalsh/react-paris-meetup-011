{:source-paths ["src"]

 :dependencies [[reagent "1.2.0"]
                [re-frame "1.3.0"]
                [org.clojure/core.async "1.6.681"]]

 :dev-http {8080 "public"}

 :builds
 {:app {:target :browser
        :output-dir "public/js"
        :asset-path "/js"
        :modules {:main {:entries [infinite-scroll.core]
                         :init-fn infinite-scroll.core/init}}
        :devtools {:after-load infinite-scroll.core/reload}}}}
